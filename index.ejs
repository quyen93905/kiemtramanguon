<!DOCTYPE html>
<html>
  <head>
    <title>user1</title>
    <link rel='stylesheet' href='/public/stylesheets/style.css' />
  </head>
  <body>
    <h1>Danh Sách User1</h1>
    <a href="/form-add"><button type="submit">Thêm Mới</button></a>
    <table border="1">
      <tr>
        <th>LatLong</th>
        <th>Role</th>
        <th>OpenHour</th>
        <th>DefaultAccount </th>
        <th>TokenLogin</th>
        <th>DeviceID</th>
        <th>FcmTokens</th>
        <th>Acction</th>
      </tr>
      <% userfours.forEach(item => {%>
        <tr>
          <td><%= item._id %></td>
          <td><%= item.Role %></td>
          <td><%= item.OpenHour %></td>
          <td><%= item.DefaultAccount %></td>
          <td><%= item.TokenLogin %></td>
          <td><%= item.DeviceID %></td>
          <td><%= item.FcmTokens %></td>
          <td><a href="/form-update/<%= item._id %>">Update</a>||<a href="/form-delete/<%= item._id %>">Delete</a></td>
        </tr>
      <%})%>
    </table>
  </body>
</html>
